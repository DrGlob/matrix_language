let base = 1;
let decision = if base == 1 then 10 else 0;

let scoped = let temp = 5 in temp * decision;

let numbers = [1, 2, 3, 4];
let doubled = numbers.map { x -> x * 2 };
let sum = numbers.reduce(0) { acc, x -> acc + x };

let more = [10, 20, 30, 40];
let paired = numbers.zip(more);
let parts = paired.unzip();

print(decision);
print(scoped);
print(doubled);
print(sum);
print(parts.first);
print(parts.second);
